.slidebox
	.slide-menu.skin01#navigator
		.bg
		ul.btn-list
			-var items=['找路线','找攻略','定制吧','找同伴','找领队','找装备','找知识','全程管家']
			-for(var i=0;i<items.length;i++)
				case i
					when 0
						li
							a.active(href="./find_line.html")= items[i]
					when 1
						li
							a(href="./find_strategy.html")= items[i]
					when 2
						li
							a(href="./find_line.html")= items[i]
					when 3
						li
							a(href="./find_companion.html")= items[i]
					when 4
						li
							a(href="./find_leader.html")= items[i]
					when 5
						li
							a(href="./find_equipment.html")= items[i]
					when 6
						li
							a(href="./find_knowledge.html")= items[i]
					default
						li
							a(href="./afa_steward.html")= items[i]